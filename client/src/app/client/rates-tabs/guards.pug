form(name='rateGuardsForm')
	.section-form
		p.form-section-glarus
			| {{ 'client.rates.staff' | translate }}
		// Guard
		div.an-material
			md-input-container
				label {{ 'client.rates.function' | translate}}
				input(value="{{ 'client.rates.guard' | translate}}" ng-disabled='true')
			md-input-container
				label {{ 'client.rates.rateDay' | translate}}
				input(ng-model='rateForm.rates.GUARD.inputParameters.rateDay' type="number")
		// Guard shit manager.
		div.an-material
			md-input-container
				label {{ 'client.rates.function' | translate}}
				input(value="{{ 'client.rates.guardShiftManager' | translate}}" ng-disabled='true')
			md-input-container
				label {{ 'client.rates.rateDay' | translate}}
				input(ng-model='rateForm.rates.GUARD_SHIFT_MANAGER.inputParameters.rateDay' type="number")
		// Guard night shift maintenance.
		div.an-material
			md-input-container
				label {{ 'client.rates.function' | translate}}
				input(value="{{ 'client.rates.guardNightShitMaintenance' | translate}}" ng-disabled='true')
			md-input-container
				label {{ 'client.rates.rateDay' | translate}}
				input(ng-model='rateForm.rates.GUARD_NIGHT_SHIFT_MAINTENANCE.inputParameters.rateDay' type="number")
		// Guard goods receipt.
		div.an-material
			md-input-container
				label {{ 'client.rates.function' | translate}}
				input(value="{{ 'client.rates.guardsGoodsReceipt' | translate}}" ng-disabled='true')
			md-input-container
				label {{ 'client.rates.rateDay' | translate}}
				input(ng-model='rateForm.rates.GUARD_GOODS_RECEIPT.inputParameters.rateDay' type="number")
		// Guard support.
		div.an-material
			md-input-container
				label {{ 'client.rates.function' | translate}}
				input(value="{{ 'client.rates.guardSupport' | translate}}" ng-disabled='true')
			md-input-container
				label {{ 'client.rates.rateDay' | translate}}
				input(ng-model='rateForm.rates.GUARD_SUPPORT.inputParameters.rateDay' type="number")
