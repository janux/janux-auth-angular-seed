form(name='rateResearchForm')
	.section-form
		p.form-section-glarus
			| {{ 'client.rates.researchRates' | translate }}

		// Research legal.
		div.an-material
			md-input-container
				label {{ 'client.rates.function' | translate}}
				input(value="{{ 'client.rates.researchLegal' | translate}}" ng-disabled='true')
			md-input-container
				label {{ 'client.rates.rateDay' | translate}}
				input(ng-model='rateForm.rates.RESEARCH_LEGAL.inputParameters.rateDay' type="number")
